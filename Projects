      "source": [
        "# Resize all 11,200 images in the train directory\n",
        "train_generator = ImageDataGenerator(rescale=1./255).flow_from_directory(train_dir,\n",
        "                                                     target_size=(96,96), batch_size=11200, color_mode='grayscale')"
      ],
      "execution_count": 11,
      "outputs": [
        {
          "output_type": "stream",
